df_text[,id] <- as.character(df_text[,id])
plot.lines <-
ggplot2::geom_text(
data = df_text,
ggplot2::aes_string(
label=label,
x = id,
y = start,
col = name_col,
size = name_size,
alpha = name_alpha,
fontface =name_fontface
